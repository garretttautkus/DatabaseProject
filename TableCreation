CREATE TABLE ORGANIZATION
(
  OName NVARCHAR(255) NOT NULL,
  OAddress NVARCHAR(255) NOT NULL,
  OCity NVARCHAR(255) NOT NULL,
  OState NVARCHAR(255) NOT NULL,
  OName NVARCHAR(255)  NOT NULL,
  OAddress NVARCHAR(255) NOT NULL,
  OCity NVARCHAR(14) NOT NULL,
  OState VARCHAR NOT NULL,
  OZip INT NOT NULL,
  OEmail NVARCHAR(255) NOT NULL,
  OPhone CHAR(10) NOT NULL,
  OID INT NOT NULL,
  PRIMARY KEY (OID)
);
CREATE TABLE HOTEL
(
  HMaxMeetSize VARCHAR NOT NULL,
  HName VARCHAR NOT NULL,
  HState VARCHAR NOT NULL,
  HZip INT NOT NULL,
  HAddress VARCHAR NOT NULL,
  HContactPhone INT NOT NULL,
  HPhone INT NOT NULL,
  HNumRooms INT NOT NULL,
  HCity VARCHAR NOT NULL,
  PRIMARY KEY (HName)
);
CREATE TABLE CONFERENCE
(
  CName VARCHAR NOT NULL,
  CID INT NOT NULL,
  CStartDate DATE NOT NULL,
  CEndDate DATE NOT NULL,
  CCity VARCHAR NOT NULL,
  CFee INT NOT NULL,
  CAttendance INT NOT NULL,
  CBudget INT NOT NULL,
  OID INT NOT NULL,
  HName VARCHAR NOT NULL,
  PRIMARY KEY (CID),
  FOREIGN KEY (OID) REFERENCES ORGANIZATION(OID),
  FOREIGN KEY (HName) REFERENCES HOTEL(HName)
);
CREATE TABLE ATTENDEE
(
  AName VARCHAR NOT NULL,
  APhone INT NOT NULL,
  ADiscount INT NOT NULL,
  AID INT NOT NULL,
  AEmail VARCHAR NOT NULL,
  AFeePaid TINYINT NOT NULL,
  CID INT NOT NULL,
  HName VARCHAR NOT NULL,
  PRIMARY KEY (AID),
  FOREIGN KEY (CID) REFERENCES CONFERENCE(CID),
  FOREIGN KEY (HName) REFERENCES HOTEL(HName)
);






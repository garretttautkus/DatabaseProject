CREATE TABLE ORGANIZATION
(
  OName INT NOT NULL,
  OAddress INT NOT NULL,
  OCity INT NOT NULL,
  OState INT NOT NULL,
  OZip INT NOT NULL,
  OEmail INT NOT NULL,
  OPhone INT NOT NULL,
  OID INT NOT NULL,
  PRIMARY KEY (OID)
);
CREATE TABLE HOTEL
(
  HMaxMeetSize INT NOT NULL,
  HName NVARCHAR NOT NULL,
  HState NVARCHAR NOT NULL,
  HZip INT NOT NULL,
  HAddress NVARCHAR NOT NULL,
  HContactPhone INT NOT NULL,
  HPhone INT NOT NULL,
  HNumRooms INT NOT NULL,
  HCity NVARCHAR NOT NULL,
  PRIMARY KEY (HName)
);
CREATE TABLE CONFERENCE
(
  CName NVARCHAR NOT NULL,
  CID INT NOT NULL,
  CStartDate DATE NOT NULL,
  CEndDate DATE NOT NULL,
  CCity NVARCHAR NOT NULL,
  CFee INT NOT NULL,
  CAttendance INT NOT NULL,
  CBudget INT NOT NULL,
  OID INT NOT NULL,
  HName NVARCHAR NOT NULL,
  PRIMARY KEY (CID),
  FOREIGN KEY (OID) REFERENCES ORGANIZATION(OID),
  FOREIGN KEY (HName) REFERENCES HOTEL(HName)
);
CREATE TABLE ATTENDEE
(
  AName INT NOT NULL,
  APhone INT NOT NULL,
  ADiscount INT NOT NULL,
  AID INT NOT NULL,
  AEmail INT NOT NULL,
  AFeePaid INT NOT NULL,
  CID INT NOT NULL,
  HName INT NOT NULL,
  PRIMARY KEY (AID),
  FOREIGN KEY (CID) REFERENCES CONFERENCE(CID),
  FOREIGN KEY (HName) REFERENCES HOTEL(HName)
);







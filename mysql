CREATE TABLE ORGANIZATION
(
  OName NVARCHAR  NOT NULL,
  OAddress NVARCHAR NOT NULL,
  OCity NVARCHAR NOT NULL,
  OState NVARCHAR NOT NULL,
  OZip INT NOT NULL,
  OEmail NVARCHAR NOT NULL,
  OPhone CHAR(10) NOT NULL,
  OID INT NOT NULL,
  PRIMARY KEY (OID)
);
CREATE TABLE HOTEL
(
  HMaxMeetSize INT NOT NULL,
  HName INT NOT NULL,
  HState INT NOT NULL,
  HZip INT NOT NULL,
  HAddress INT NOT NULL,
  HContactPhone INT NOT NULL,
  HPhone INT NOT NULL,
  HNumRooms INT NOT NULL,
  HCity INT NOT NULL,
  PRIMARY KEY (HName)
);
CREATE TABLE CONFERENCE
(
  CName INT NOT NULL,
  CID INT NOT NULL,
  CStartDate INT NOT NULL,
  CEndDate INT NOT NULL,
  CCity INT NOT NULL,
  CFee INT NOT NULL,
  CAttendance INT NOT NULL,
  CBudget INT NOT NULL,
  OID INT NOT NULL,
  HName INT NOT NULL,
  PRIMARY KEY (CID),
  FOREIGN KEY (OID) REFERENCES ORGANIZATION(OID),
  FOREIGN KEY (HName) REFERENCES HOTEL(HName)
);
CREATE TABLE ATTENDEE
(
  AName INT NOT NULL,
  APhone INT NOT NULL,
  ADiscount INT NOT NULL,
  AID INT NOT NULL,
  AEmail INT NOT NULL,
  AFeePaid INT NOT NULL,
  CID INT NOT NULL,
  HName INT NOT NULL,
  PRIMARY KEY (AID),
  FOREIGN KEY (CID) REFERENCES CONFERENCE(CID),
  FOREIGN KEY (HName) REFERENCES HOTEL(HName)
);






